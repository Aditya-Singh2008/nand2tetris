/**
 * 8-way demultiplexor:
 * [a, b, c, d, e, f, g, h] = [in, 0,  0,  0,  0,  0,  0,  0] if sel = 000
 *                            [0, in,  0,  0,  0,  0,  0,  0] if sel = 001
 *                            [0,  0, in,  0,  0,  0,  0,  0] if sel = 010
 *                            [0,  0,  0, in,  0,  0,  0,  0] if sel = 011
 *                            [0,  0,  0,  0, in,  0,  0,  0] if sel = 100
 *                            [0,  0,  0,  0,  0, in,  0,  0] if sel = 101
 *                            [0,  0,  0,  0,  0,  0, in,  0] if sel = 110
 *                            [0,  0,  0,  0,  0,  0,  0, in] if sel = 111
 */
CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in=in, sel=sel[2], a=top, b=bottom);

    DMux(in=top, sel=sel[1], a=top1, b=top2);
    DMux(in=bottom, sel=sel[1], a=bottom1, b=bottom2);


    DMux(in=top1, sel=sel[0], a=a, b=b);
    DMux(in=top2, sel=sel[0], a=c, b=d);
    DMux(in=bottom1, sel=sel[0], a=e, b=f);
    DMux(in=bottom2, sel=sel[0], a=g, b=h);
}